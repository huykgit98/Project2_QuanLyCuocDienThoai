@model IEnumerable<WebMVC.Models.DETAIL>
@{
    ViewBag.Title = "Detail";

    Layout = "~/Views/Shared/_Layout.cshtml";

}
<div id="page-wrapper">
    <div class="row">
        <div class="row">
            <div class="col-lg-12">
                <h4 style="margin-top: 23px; font-size: 40px; color: #4800ff">
                    Chi Tiết Sử Dụng

                </h4>
            </div>
            <!-- /.col-lg-12 -->

        </div>
        <div class="row">
           
            <table class="table table-striped table-bordered table-hover">
                <tr class="info">
                    <th>
                        SĐT
                    </th>
                    <th>
                        Thời gian bắt đầu
                    </th>
                    <th>
                        Thời gian kết thúc
                    </th>
                    <th>
                        Số phút 7h-23h
                    </th>
                    <th>
                        Số phút 23-7h
                    </th>
                    <th>
                        Cước phí
                    </th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.SIM.phone)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.time_start)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.time_stop)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.minute_7hto23h)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.minute_23hto7h)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.fare)
                        </td>
                        
                    </tr>
                }
            </table>
        </div>
    </div>
</div>
